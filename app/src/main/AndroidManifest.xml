<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.untilled.roadcapture">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.GlobalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoadCapture">
        <activity android:name=".application.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".application.MainActivity"
            android:screenOrientation="portrait">

        </activity>

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="qwpjxdji3c" />
    </application>

</manifest>