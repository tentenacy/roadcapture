<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start">

    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/imageview_my_studio_background"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            motion:layout_constraintDimensionRatio="4:3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/view_my_studio_background_overlay"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageview_my_studio_background"
            motion:layout_constraintEnd_toEndOf="@+id/imageview_my_studio_background"
            motion:layout_constraintStart_toStartOf="@+id/imageview_my_studio_background"
            motion:layout_constraintTop_toTopOf="@+id/imageview_my_studio_background" />

        <Constraint
            android:id="@+id/imageview_my_studio_profile"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageview_my_studio_background"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/imageview_my_studio_background"
            motion:layout_constraintVertical_bias="0.7" />

        <Constraint
            android:id="@+id/textview_my_studio_profile_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            motion:layout_constraintStart_toEndOf="@id/imageview_my_studio_profile"
            motion:layout_constraintTop_toTopOf="@id/imageview_my_studio_profile" />

        <Constraint
            android:id="@+id/imageview_my_studio_modification"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:padding="4dp"
            android:layout_marginBottom="16dp"
            motion:layout_constraintStart_toEndOf="@id/textview_my_studio_profile_name"
            motion:layout_constraintTop_toTopOf="@id/textview_my_studio_profile_name"
            motion:layout_constraintBottom_toBottomOf="@id/textview_my_studio_profile_name"/>

        <Constraint
            android:id="@+id/textview_my_studio_profile_name_motion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:alpha="0"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />

        <Constraint
            android:id="@+id/textview_my_studio_profile_description"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="@id/textview_my_studio_profile_name"
            motion:layout_constraintTop_toBottomOf="@id/textview_my_studio_profile_name" />

        <Constraint
            android:id="@+id/view"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/imageview_my_studio_profile"
            motion:layout_constraintTop_toBottomOf="@+id/imageview_my_studio_profile" />

        <Constraint
            android:id="@+id/textview_my_studio_follower"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            motion:layout_constraintBottom_toBottomOf="@id/imageview_my_studio_background"
            motion:layout_constraintStart_toStartOf="@+id/imageview_my_studio_profile"
            motion:layout_constraintTop_toBottomOf="@+id/imageview_my_studio_profile" />

        <Constraint
            android:id="@+id/textview_my_studio_follower_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            motion:layout_constraintHorizontal_chainStyle="spread"
            motion:layout_constraintStart_toEndOf="@+id/textview_my_studio_follower"
            motion:layout_constraintTop_toTopOf="@+id/textview_my_studio_follower" />

        <Constraint
            android:id="@+id/textview_my_studio_following"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="96dp"
            motion:layout_constraintStart_toEndOf="@+id/textview_my_studio_follower"
            motion:layout_constraintTop_toTopOf="@+id/textview_my_studio_follower" />


        <Constraint
            android:id="@+id/textview_my_studio_following_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            motion:layout_constraintStart_toEndOf="@+id/textview_my_studio_following"
            motion:layout_constraintTop_toTopOf="@+id/textview_my_studio_follower" />

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/imageview_my_studio_background"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:alpha="0"
            motion:layout_constraintDimensionRatio="4:3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/view_my_studio_background_overlay"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:alpha="0"
            motion:layout_constraintBottom_toBottomOf="@+id/imageview_my_studio_background"
            motion:layout_constraintEnd_toEndOf="@+id/imageview_my_studio_background"
            motion:layout_constraintStart_toStartOf="@+id/imageview_my_studio_background"
            motion:layout_constraintTop_toTopOf="@+id/imageview_my_studio_background" />

        <Constraint
            android:id="@+id/imageview_my_studio_profile"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:alpha="0"
            motion:layout_constraintBottom_toBottomOf="@+id/imageview_my_studio_background"
            motion:layout_constraintStart_toStartOf="@+id/imageview_my_studio_background"
            motion:layout_constraintTop_toTopOf="@+id/imageview_my_studio_background"
            motion:layout_constraintVertical_bias="0.7" />

        <Constraint
            android:id="@+id/textview_my_studio_profile_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:alpha="0"
            motion:layout_constraintStart_toEndOf="@id/imageview_my_studio_profile"
            motion:layout_constraintTop_toTopOf="@id/imageview_my_studio_profile" />

        <Constraint
            android:id="@+id/imageview_my_studio_modification"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:padding="4dp"
            android:alpha="0"
            android:layout_marginBottom="16dp"
            motion:layout_constraintStart_toEndOf="@id/textview_my_studio_profile_name"
            motion:layout_constraintTop_toTopOf="@id/textview_my_studio_profile_name"
            motion:layout_constraintBottom_toBottomOf="@id/textview_my_studio_profile_name"/>

        <Constraint
            android:id="@+id/textview_my_studio_profile_name_motion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginBottom="12dp"
            android:alpha="1"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />

        <Constraint
            android:id="@+id/textview_my_studio_profile_description"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="16dp"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@id/textview_my_studio_profile_name"
            motion:layout_constraintTop_toBottomOf="@id/textview_my_studio_profile_name" />

        <Constraint
            android:id="@+id/view"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="@+id/imageview_my_studio_profile"
            motion:layout_constraintTop_toBottomOf="@+id/imageview_my_studio_profile" />

        <Constraint
            android:id="@+id/textview_my_studio_follower"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:alpha="0"
            motion:layout_constraintBottom_toBottomOf="@id/imageview_my_studio_background"
            motion:layout_constraintStart_toStartOf="@+id/imageview_my_studio_profile"
            motion:layout_constraintTop_toBottomOf="@+id/imageview_my_studio_profile" />

        <Constraint
            android:id="@+id/textview_my_studio_follower_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:alpha="0"
            motion:layout_constraintHorizontal_chainStyle="spread"
            motion:layout_constraintStart_toEndOf="@+id/textview_my_studio_follower"
            motion:layout_constraintTop_toTopOf="@+id/textview_my_studio_follower" />

        <Constraint
            android:id="@+id/textview_my_studio_following"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="96dp"
            android:alpha="0"
            motion:layout_constraintStart_toEndOf="@+id/textview_my_studio_follower"
            motion:layout_constraintTop_toTopOf="@+id/textview_my_studio_follower" />


        <Constraint
            android:id="@+id/textview_my_studio_following_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:alpha="0"
            motion:layout_constraintStart_toEndOf="@+id/textview_my_studio_following"
            motion:layout_constraintTop_toTopOf="@+id/textview_my_studio_follower" />

    </ConstraintSet>
</MotionScene>